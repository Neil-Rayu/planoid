    "eslint-plugin-svelte3": "^3.2.1",
    "mysql": "^2.18.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.49.0",
    "svelte": "^3.44.0",
    "svelte-adapter-firebase": "^0.13.1",
    "svelte-check": "^2.2.6",
